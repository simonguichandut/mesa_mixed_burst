! Relax to new luminosity 

&star_job    

  load_saved_model = .true.
  saved_model_name = 'models/ns_env_56Fe.mod' 

  save_model_when_terminate = .true.
  save_model_filename = 'models/ns_env_relaxed.mod'

  set_initial_model_number = .true.
  initial_model_number = 0
  set_initial_age = .true.
  initial_age = 0

  relax_L_center = .true.
  new_L_center = 1.9e34 ! erg/s (directory name)
  dlgL_per_step = 1d-2

  pgstar_flag = .false.

/ ! end of star_job namelist

&controls      

  !max_age = 1d0 ! relax for a year
  max_model_number = 100 ! relax for 100 models

  ! Resolution
  varcontrol_target = 1d-3 ! default 1d-3
  mesh_delta_coeff = 1.0 ! default 1.0
  time_delta_coeff = 1d0 ! default 1d0

  photo_interval = 1000
  profile_interval = 1000
  history_interval = 1000
  terminal_interval = 10
  write_header_frequency = 50

/ ! end of controls namelist
